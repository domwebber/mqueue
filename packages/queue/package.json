{
  "name": "@mqueue/queue",
  "version": "0.0.1",
  "type": "module",
  "description": "A simple queue interface with support for multiple backends",
  "author": "Dom Webber <dom.webber@hotmail.com>",
  "contributors": [
    "Dom Webber <dom.webber@hotmail.com>"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "queue",
    "adapter",
    "amqplib",
    "rhea",
    "sqs",
    "sqs-consumer",
    "azure-service-bus"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src --no-config --dts --format esm,cjs --cjsInterop --minify --sourcemap --splitting --treeshake",
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test src/__tests__/**/*.test.ts src/__tests__/*.test.ts",
    "lint:publint": "publint"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": false
  }
}
