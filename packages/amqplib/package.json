{
  "name": "@mqueue/amqplib",
  "version": "0.0.1",
  "type": "module",
  "description": "MQueue adapter for amqplib AMQP v0.9.1",
  "author": "Dom Webber <dom.webber@hotmail.com>",
  "contributors": [
    "Dom Webber <dom.webber@hotmail.com>"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "queue",
    "mqueue",
    "amqplib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src --no-config --dts --format esm,cjs --cjsInterop --minify --sourcemap --splitting --treeshake",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": false
  },
  "peerDependencies": {
    "@mqueue/queue": "workspace:*",
    "amqplib": "^0.10.8"
  },
  "devDependencies": {
    "@mqueue/queue": "workspace:*",
    "@types/amqplib": "^0.10.7",
    "amqplib": "^0.10.8"
  }
}
