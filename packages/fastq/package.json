{
  "name": "@mqueue/fastq",
  "version": "0.0.1",
  "type": "module",
  "description": "MQueue adapter for fastq",
  "author": "Dom Webber <dom.webber@hotmail.com>",
  "contributors": [
    "Dom Webber <dom.webber@hotmail.com>"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "queue",
    "mqueue",
    "fastq"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src --no-config --dts --format esm,cjs --cjsInterop --minify --sourcemap --splitting --treeshake",
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test src/__tests__/**/*.test.ts src/__tests__/*.test.ts",
    "lint:publint": "publint"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": false
  },
  "peerDependencies": {
    "@mqueue/queue": "workspace:*"
  },
  "devDependencies": {
    "@mqueue/queue": "workspace:*",
    "testcontainers": "^11.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "fastq": "^1.19.1"
  }
}
